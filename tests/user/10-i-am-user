#!/bin/execlineb -P
if {
    getcwd CWD
    import CWD
    s6-logcmd
    s6-test $CWD = /tmp
}
if {
    backtick -n USER { id -un }
    import USER
    s6-logcmd
    s6-test $USER = docker
}
if {
    backtick -n GRP { id -gn }
    import GRP
    s6-logcmd
    s6-test $GRP = docker
}
if {
    import -i HOME
    s6-logcmd
    s6-test $HOME = /home/docker
}
if {
    import -D "" BAR
    s6-logcmd
    s6-test $BAR = FOO
}
